<div class="post nixmash-post">
    <h3><a target="_blank" href="${post.postLink}">${post.postTitle}</a>
    </h3>
    <div class="post-content">${post.postContent}</div>
    <img class="nixmash-feature-img" alt="" src="${post.postImage}"/>

    <div class="post-date">
        #parse("posts/inc/post_date.vm")
    </div>
    <div class="nixmash-tag"><a href="http://nixmash.com" target="_blank">
        <img src="/images/posts/nixmashtag.png" alt=""/></a></div>
</div>
